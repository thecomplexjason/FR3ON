local plot = workspace.Plots:FindFirstChild("Plot1") -- أو أي موديل تعرفه

if plot then
	local sign = plot:FindFirstChild("PlotSign")
	if sign then
		local gui = sign:FindFirstChildWhichIsA("SurfaceGui")
		if gui then
			local frame = gui:FindFirstChildWhichIsA("Frame")
			if frame then
				local textLabel = frame:FindFirstChildWhichIsA("TextLabel")
				if textLabel then
					while true do
						print("DEBUG TEXT:", textLabel.Text)
						wait(1)
					end
				end
			end
		end
	end
end
