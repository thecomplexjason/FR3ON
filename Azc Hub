local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local ScreenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
ScreenGui.Name = "TeleportGui"

local button = Instance.new("TextButton")
button.Name = "InstantSteal"
button.Size = UDim2.new(0, 140, 0, 40)
button.Position = UDim2.new(0.5, -70, 0.5, -20)
button.Text = "AZC HUB â–¼"
button.BackgroundColor3 = Color3.fromRGB(100, 200, 50)
button.TextColor3 = Color3.new(1, 1, 1)
button.Font = Enum.Font.SourceSansBold
button.TextSize = 20
button.Draggable = true
button.Parent = ScreenGui

local dropDown = Instance.new("Frame")
dropDown.Size = UDim2.new(1, 0, 0, 80)
dropDown.Position = UDim2.new(0, 0, 1, 0)
dropDown.BackgroundColor3 = Color3.fromRGB(200, 70, 70)
dropDown.Visible = false
dropDown.ClipsDescendants = true
dropDown.Parent = button

local option1 = Instance.new("TextButton")
option1.Size = UDim2.new(1, 0, 0, 40)
option1.Position = UDim2.new(0, 0, 0, 0)
option1.Text = "Instant Steal"
option1.BackgroundColor3 = Color3.fromRGB(100, 200, 50)
option1.TextColor3 = Color3.new(1, 1, 1)
option1.Font = Enum.Font.SourceSansBold
option1.TextSize = 18
option1.Parent = dropDown

local option2 = Instance.new("TextButton")
option2.Size = UDim2.new(1, 0, 0, 40)
option2.Position = UDim2.new(0, 0, 0, 40)
option2.Text = "UP"
option2.BackgroundColor3 = Color3.fromRGB(100, 200, 50)
option2.TextColor3 = Color3.new(1, 1, 1)
option2.Font = Enum.Font.SourceSansBold
option2.TextSize = 18
option2.Parent = dropDown

local dropVisible = false
button.MouseButton1Click:Connect(function()
    dropVisible = not dropVisible
    dropDown.Visible = dropVisible
end)

option1.MouseButton1Click:Connect(function()
    if humanoidRootPart then
        humanoidRootPart.CFrame = CFrame.new(humanoidRootPart.Position.X, 90.53, humanoidRootPart.Position.Z)
    end
end)


option2.MouseButton1Click:Connect(function()
    if humanoidRootPart then
        humanoidRootPart.CFrame = CFrame.new(humanoidRootPart.Position.X, 130.53, humanoidRootPart.Position.Z)
    end
end)
