local originalText = esp.textLabel.Text
if originalText ~= "Empty Base" then
	-- شيل 's Base من آخر النص
	local nameOnly = originalText:match("^(.-)'s Base$")
	if nameOnly then
		esp.label.Text = nameOnly
	else
		esp.label.Text = originalText -- fallback لو الشكل مختلف
	end
else
	esp.label.Text = originalText
end
