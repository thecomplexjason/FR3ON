local speedEnabled = false
local normalSpeed = 16
local boostedSpeed = 50

local speedLabel = Instance.new("TextLabel", playerSection)
speedLabel.Size = UDim2.new(0.6, 0, 0, 40)
speedLabel.Position = UDim2.new(0, 10, 0, 190)
speedLabel.Text = "سرعة عالية"
speedLabel.Font = Enum.Font.SourceSansBold
speedLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
speedLabel.BackgroundTransparency = 1
speedLabel.TextScaled = true
speedLabel.TextXAlignment = Enum.TextXAlignment.Left

local speedSwitch = Instance.new("TextButton", playerSection)
speedSwitch.Size = UDim2.new(0, 60, 0, 30)
speedSwitch.Position = UDim2.new(1, -70, 0, 195)
speedSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
speedSwitch.Text = ""
speedSwitch.AutoButtonColor = false
speedSwitch.BorderSizePixel = 0

local speedCircle = Instance.new("Frame", speedSwitch)
speedCircle.Size = UDim2.new(0, 26, 0, 26)
speedCircle.Position = UDim2.new(0, 2, 0, 2)
speedCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
speedCircle.BorderSizePixel = 0
Instance.new("UICorner", speedSwitch).CornerRadius = UDim.new(1, 0)
Instance.new("UICorner", speedCircle).CornerRadius = UDim.new(1, 0)

local function updateSpeed(state)
	speedEnabled = state
	local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
	if not humanoid then return end
	
	if state then
		speedSwitch.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
		speedCircle:TweenPosition(UDim2.new(1, -28, 0, 2), "Out", "Quad", 0.2, true)
		humanoid.WalkSpeed = boostedSpeed
	else
		speedSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
		speedCircle:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Quad", 0.2, true)
		humanoid.WalkSpeed = normalSpeed
	end
end

speedSwitch.MouseButton1Click:Connect(function()
	updateSpeed(not speedEnabled)
end)

player.CharacterAdded:Connect(function(char)
	char:WaitForChild("Humanoid")
	task.delay(0.5, function()
		updateSpeed(speedEnabled)
	end)
end)
