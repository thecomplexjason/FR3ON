local strongholdTeleportButton = Instance.new("TextButton", stealSection)
strongholdTeleportButton.Size = UDim2.new(0.8, 0, 0, 40)
strongholdTeleportButton.Position = UDim2.new(0, 10, 0, 160) -- تحت زر الانتقال إلى الحداد
strongholdTeleportButton.Text = "الانتقال إلى القلعة"
strongholdTeleportButton.Font = Enum.Font.SourceSansBold
strongholdTeleportButton.TextScaled = true
strongholdTeleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
strongholdTeleportButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
strongholdTeleportButton.BorderSizePixel = 0
Instance.new("UICorner", strongholdTeleportButton).CornerRadius = UDim.new(0, 8)

strongholdTeleportButton.MouseButton1Click:Connect(function()
    local targetPart = workspace:FindFirstChild("Map")
        and workspace.Map:FindFirstChild("Landmarks")
        and workspace.Map.Landmarks:FindFirstChild("Stronghold")
        
    if targetPart then
        local char = Players.LocalPlayer.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        if hrp then

            hrp.CFrame = targetPart.PrimaryPart 
                and targetPart.PrimaryPart.CFrame + Vector3.new(0, 10, 0)
                or targetPart:GetModelCFrame() + Vector3.new(0, 10, 0)
        end
    else
playSound("Azc Notifications", "https://github.com/thecomplexjason/FR3ON/raw/refs/heads/main/Azc%20Notifications.mp3")
game.StarterGui:SetCore("SendNotification", {
    Title = "AZC HUB",
    Text = "لم يتم العثور علي القلعة يرجى تشغيل ميزة تحميل الماب اولا",
    Duration = 999,
    Button1 = "Script By FR3ON",
    Icon = azcIcon
})
    end
end)
