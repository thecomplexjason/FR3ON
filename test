startButton.MouseButton1Click:Connect(function()
	for _, section in pairs(contentFrame:GetChildren()) do
		if section:IsA("ScrollingFrame") then section.Visible = false end
	end
	startSection.Visible = true
end)

playerButton.MouseButton1Click:Connect(function()
	for _, section in pairs(contentFrame:GetChildren()) do
		if section:IsA("ScrollingFrame") then section.Visible = false end
	end
	playerSection.Visible = true
end)

infButton.MouseButton1Click:Connect(function()
	for _, section in pairs(contentFrame:GetChildren()) do
		if section:IsA("ScrollingFrame") then section.Visible = false end
	end
	infSection.Visible = true
end)
stealButton.MouseButton1Click:Connect(function()
	for _, section in pairs(contentFrame:GetChildren()) do
		if section:IsA("ScrollingFrame") then section.Visible = false end
	end
	stealSection.Visible = true
end)

local visible = false

local function slideIn()
	TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {
		Position = UDim2.new(0, 200, 0.5, -200)
	}):Play()
end

local function slideOut()
	TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {
		Position = UDim2.new(0, -510, 0.5, -150)
	}):Play()
end

toggleButton.MouseButton1Click:Connect(function()
	if visible then slideOut() else slideIn() end
	visible = not visible
end)

hideBtn.MouseButton1Click:Connect(function()
	slideOut()
	visible = false
end)

closeBtn.MouseButton1Click:Connect(function()
	slideOut()
	visible = false
	wait(0.4)
	screenGui.Enabled = false
end)
