local LOBBY_PLACE_ID = 79546208627805
local currentPlaceId = game.PlaceId
local inGame = false

if not isfolder('Azc Hub') then
    makefolder('Azc Hub')
end

local function saveImage(name, link)
    local filePath = "Azc Hub/".. name ..".png"
    if not isfile(filePath) then
        writefile(filePath, game:HttpGet(link))
    end
    return getcustomasset(filePath)
end

local function playSound(name, link)
    local filePath = "Azc Hub/".. name ..".mp3"
    if not isfile(filePath) then
        writefile(filePath, game:HttpGet(link))
    end
    local sound = Instance.new("Sound")
    sound.SoundId = getcustomasset(filePath)
    sound.Volume = 2
    sound.Parent = workspace
    sound:Play()
end

local azcIcon = saveImage("Azc", "https://raw.githubusercontent.com/thecomplexjason/FR3ON/refs/heads/main/Azc.webp")

