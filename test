local teleportCampButton = Instance.new("TextButton", stealSection)
teleportCampButton.Size = UDim2.new(0.8, 0, 0, 40)
teleportCampButton.Position = UDim2.new(0, 10, 0, 10)
teleportCampButton.Text = "الانتقال إلى المخيم"
teleportCampButton.Font = Enum.Font.SourceSansBold
teleportCampButton.TextScaled = true
teleportCampButton.TextColor3 = Color3.fromRGB(255, 255, 255)
teleportCampButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
teleportCampButton.BorderSizePixel = 0
Instance.new("UICorner", teleportCampButton).CornerRadius = UDim.new(0, 8)

teleportCampButton.MouseButton1Click:Connect(function()
    local targetPart = workspace:FindFirstChild("Map")
        and workspace.Map:FindFirstChild("Campground")
        and workspace.Map.Campground:FindFirstChild("CraftingBench")
        and workspace.Map.Campground.CraftingBench:FindFirstChild("GreenGemPile")
        and workspace.Map.Campground.CraftingBench.GreenGemPile:FindFirstChild("1")
        and workspace.Map.Campground.CraftingBench.GreenGemPile["1"]:FindFirstChild("Quarter")
        and workspace.Map.Campground.CraftingBench.GreenGemPile["1"].Quarter:FindFirstChild("Main")
        
    if targetPart then
        local char = Players.LocalPlayer.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        if hrp then
            hrp.CFrame = targetPart.CFrame + Vector3.new(0, 10, 0)
        end
    else
playSound("Azc Notifications", "https://github.com/thecomplexjason/FR3ON/raw/refs/heads/main/Azc%20Notifications.mp3")
game.StarterGui:SetCore("SendNotification", {
    Title = "AZC HUB",
    Text = "لم يتم العثور علي المخيم يرجى تشغيل ميزة تحميل الماب اولا",
    Duration = 999,
    Button1 = "Script By FR3ON",
    Icon = azcIcon
})
    end
end)
