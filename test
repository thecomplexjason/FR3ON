local flyEnabled = false
local flySpeed = 50
local flyConnection

local flyLabel = Instance.new("TextLabel", playerSection)
flyLabel.Size = UDim2.new(0.6, 0, 0, 40)
flyLabel.Position = UDim2.new(0, 10, 0, 130)
flyLabel.Text = "طيران"
flyLabel.Font = Enum.Font.SourceSansBold
flyLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
flyLabel.BackgroundTransparency = 1
flyLabel.TextScaled = true
flyLabel.TextXAlignment = Enum.TextXAlignment.Left

local flySwitch = Instance.new("TextButton", playerSection)
flySwitch.Size = UDim2.new(0, 60, 0, 30)
flySwitch.Position = UDim2.new(1, -70, 0, 135)
flySwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
flySwitch.Text = ""
flySwitch.AutoButtonColor = false
flySwitch.BorderSizePixel = 0

local flyCircle = Instance.new("Frame", flySwitch)
flyCircle.Size = UDim2.new(0, 26, 0, 26)
flyCircle.Position = UDim2.new(0, 2, 0, 2)
flyCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
flyCircle.BorderSizePixel = 0
Instance.new("UICorner", flySwitch).CornerRadius = UDim.new(1, 0)
Instance.new("UICorner", flyCircle).CornerRadius = UDim.new(1, 0)

local function enableFly()
	local char = player.Character or player.CharacterAdded:Wait()
	local hrp = char:WaitForChild("HumanoidRootPart")
	local humanoid = char:WaitForChild("Humanoid")

	humanoid.PlatformStand = true

	flyConnection = RunService.RenderStepped:Connect(function()
		local moveDir = Vector3.new(0,0,0)

		if UIS:IsKeyDown(Enum.KeyCode.W) then
			moveDir = moveDir + (workspace.CurrentCamera.CFrame.LookVector)
		end
		if UIS:IsKeyDown(Enum.KeyCode.S) then
			moveDir = moveDir - (workspace.CurrentCamera.CFrame.LookVector)
		end
		if UIS:IsKeyDown(Enum.KeyCode.A) then
			moveDir = moveDir - (workspace.CurrentCamera.CFrame.RightVector)
		end
		if UIS:IsKeyDown(Enum.KeyCode.D) then
			moveDir = moveDir + (workspace.CurrentCamera.CFrame.RightVector)
		end

		if moveDir.Magnitude > 0 then
			hrp.Velocity = moveDir.Unit * flySpeed
		else
			hrp.Velocity = Vector3.new(0,0,0)
		end
	end)
end

local function disableFly()
	if flyConnection then
		flyConnection:Disconnect()
		flyConnection = nil
	end

	local char = player.Character
	if char then
		local humanoid = char:FindFirstChildWhichIsA("Humanoid")
		local hrp = char:FindFirstChild("HumanoidRootPart")
		if humanoid then
			humanoid.PlatformStand = false
		end
		if hrp then
			hrp.Velocity = Vector3.new(0,0,0)
		end
	end
end

local function updateFly(state)
	flyEnabled = state
	if state then
		flySwitch.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
		flyCircle:TweenPosition(UDim2.new(1, -28, 0, 2), "Out", "Quad", 0.2, true)
		enableFly()
	else
		flySwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
		flyCircle:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Quad", 0.2, true)
		disableFly()
	end
end

flySwitch.MouseButton1Click:Connect(function()
	updateFly(not flyEnabled)
end)

player.CharacterAdded:Connect(function()
	task.delay(1, function()
		if flyEnabled then
			enableFly()
		end
	end)
end)
