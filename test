local safeTeleportButton = Instance.new("TextButton", stealSection)
safeTeleportButton.Size = UDim2.new(0.8, 0, 0, 40)
safeTeleportButton.Position = UDim2.new(0, 10, 0, 60) -- تحت الزر السابق
safeTeleportButton.Text = "الانتقال إلى مكان آمن"
safeTeleportButton.Font = Enum.Font.SourceSansBold
safeTeleportButton.TextScaled = true
safeTeleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
safeTeleportButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
safeTeleportButton.BorderSizePixel = 0
Instance.new("UICorner", safeTeleportButton).CornerRadius = UDim.new(0, 8)

safeTeleportButton.MouseButton1Click:Connect(function()
    local targetPart = workspace:FindFirstChild("Map")
        and workspace.Map:FindFirstChild("Campground")
        and workspace.Map.Campground:FindFirstChild("CraftingBench")
        and workspace.Map.Campground.CraftingBench:FindFirstChild("GreenGemPile")
        and workspace.Map.Campground.CraftingBench.GreenGemPile:FindFirstChild("1")
        and workspace.Map.Campground.CraftingBench.GreenGemPile["1"]:FindFirstChild("Quarter")
        and workspace.Map.Campground.CraftingBench.GreenGemPile["1"].Quarter:FindFirstChild("Main")
        
    if targetPart then
        local char = Players.LocalPlayer.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        if hrp then
            local tempPart = Instance.new("Part")
            tempPart.Size = Vector3.new(10, 1, 10)
            tempPart.Anchored = true
            tempPart.CFrame = targetPart.CFrame + Vector3.new(0, 999, 0)
            tempPart.Parent = workspace

            hrp.CFrame = tempPart.CFrame + Vector3.new(0, 1, 0)
        end
    else
 playSound("Azc Notifications", "https://github.com/thecomplexjason/FR3ON/raw/refs/heads/main/Azc%20Notifications.mp3")
game.StarterGui:SetCore("SendNotification", {
    Title = "AZC HUB",
    Text = "لم يتم العثور علي المكان الأمن يرجى تشغيل ميزة تحميل الماب اولا",
    Duration = 999,
    Button1 = "Script By FR3ON",
    Icon = azcIcon
})
    end
end)
