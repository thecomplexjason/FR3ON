local anvilTeleportButton = Instance.new("TextButton", stealSection)
anvilTeleportButton.Size = UDim2.new(0.8, 0, 0, 40)
anvilTeleportButton.Position = UDim2.new(0, 10, 0, 110)
anvilTeleportButton.Text = "الانتقال إلى الحداد"
anvilTeleportButton.Font = Enum.Font.SourceSansBold
anvilTeleportButton.TextScaled = true
anvilTeleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
anvilTeleportButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
anvilTeleportButton.BorderSizePixel = 0
Instance.new("UICorner", anvilTeleportButton).CornerRadius = UDim.new(0, 8)

anvilTeleportButton.MouseButton1Click:Connect(function()
    local targetPart = workspace:FindFirstChild("Map")
        and workspace.Map:FindFirstChild("Landmarks")
        and workspace.Map.Landmarks:FindFirstChild("ToolWorkshop")
        and workspace.Map.Landmarks.ToolWorkshop:FindFirstChild("Functional")
        and workspace.Map.Landmarks.ToolWorkshop.Functional:FindFirstChild("AnvilHologram")
        and workspace.Map.Landmarks.ToolWorkshop.Functional.AnvilHologram:FindFirstChild("Anvil Base")
        
    if targetPart then
        local char = Players.LocalPlayer.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        if hrp then

            local forwardVector = targetPart.CFrame.LookVector
            hrp.CFrame = targetPart.CFrame + forwardVector * 30
        end
    else
playSound("Azc Notifications", "https://github.com/thecomplexjason/FR3ON/raw/refs/heads/main/Azc%20Notifications.mp3")
game.StarterGui:SetCore("SendNotification", {
    Title = "AZC HUB",
    Text = "لم يتم العثور علي الحداد يرجى تشغيل ميزة تحميل الماب اولا",
    Duration = 999,
    Button1 = "Script By FR3ON",
    Icon = azcIcon
})
    end
end)
