local visible = false

local function slideIn()
	TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {
		Position = UDim2.new(0, 200, 0.5, -200)
	}):Play()
end

local function slideOut()
	TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {
		Position = UDim2.new(0, -510, 0.5, -150)
	}):Play()
end

toggleButton.MouseButton1Click:Connect(function()
	if visible then slideOut() else slideIn() end
	visible = not visible
end)

local UserInputService = game:GetService("UserInputService")
UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if gameProcessed then return end
	if input.KeyCode == Enum.KeyCode.R then
if visible then slideOut() else slideIn() end
	visible = not visible
	end
end)


hideBtn.MouseButton1Click:Connect(function()
	slideOut()
	visible = false
end)

closeBtn.MouseButton1Click:Connect(function()
	slideOut()
	visible = false
	wait(0.4)
	screenGui.Enabled = false
end)

local function createDivider(parent, yPosition)
	local divider = Instance.new("Frame", parent)
	divider.Size = UDim2.new(1, -20, 0, 2)
	divider.Position = UDim2.new(0, 10, 0, yPosition)
	divider.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
	divider.BorderSizePixel = 0
end
