local toggleWaterButton = Instance.new("TextButton", autoSection)
toggleWaterButton.Size = UDim2.new(0.9, 0, 0, 60)
toggleWaterButton.Position = UDim2.new(0, 10, 0, 60)
toggleWaterButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
toggleWaterButton.Text = ""
toggleWaterButton.BorderSizePixel = 0

local waterCorner = Instance.new("UICorner")
waterCorner.CornerRadius = UDim.new(0, 10)
waterCorner.Parent = toggleWaterButton

local waterLabel = Instance.new("TextLabel", toggleWaterButton)
waterLabel.Size = UDim2.new(1, -80, 1, 0)
waterLabel.Position = UDim2.new(0, 15, 0, 0)
waterLabel.BackgroundTransparency = 1
waterLabel.Text = "سحب بحيرة عشوائية"
waterLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
waterLabel.TextXAlignment = Enum.TextXAlignment.Left
waterLabel.Font = Enum.Font.GothamBold
waterLabel.TextSize = 27
waterLabel.TextScaled = false

local waterIcon = Instance.new("ImageLabel", toggleWaterButton)
waterIcon.Size = UDim2.new(0, 40, 0, 40)
waterIcon.Position = UDim2.new(1, -50, 0.5, -20)
waterIcon.BackgroundTransparency = 1
waterIcon.Image = clickIcon

toggleWaterButton.MouseEnter:Connect(function()
	toggleWaterButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
end)
toggleWaterButton.MouseLeave:Connect(function()
	toggleWaterButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
end)

local grabbedPart
local originalCFrame
local isGrabbed = false

toggleWaterButton.MouseButton1Click:Connect(function()
	local map = workspace:FindFirstChild("Map")
	local waterFolder = map and map:FindFirstChild("Water")

	if not isGrabbed then
		if waterFolder then
			for _, part in ipairs(waterFolder:GetChildren()) do
				if part:IsA("BasePart") and part.Color == Color3.fromRGB(0, 0, 255) then
					grabbedPart = part
					originalCFrame = part.CFrame
					if hrp then
						part.CFrame = hrp.CFrame * CFrame.new(0, 0, -5)
					end
					isGrabbed = true
					break
				end
			end
		end

		if not isGrabbed then
			playSound("Azc Notifications", "https://github.com/thecomplexjason/FR3ON/raw/refs/heads/main/Azc%20Notifications.mp3")
			game.StarterGui:SetCore("SendNotification", {
				Title = "AZC HUB",
				Text = "لم يتم العثور علي بحيرة",
				Duration = 999,
				Button1 = "Script By FR3ON",
				Icon = azcIcon
			})
		end
	else
		if grabbedPart and originalCFrame then
			grabbedPart.CFrame = originalCFrame
			isGrabbed = false
		end
	end
end)
-----------------------------------------------------------------------------
end
