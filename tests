local function playSound(name, link)
    local filePath = "Azc Hub/".. name ..".mp3"
    if not isfile(filePath) then
        writefile(filePath, game:HttpGet(link))
    end
    local soundName = name..".mp3"
    local SoundSFX = Instance.new("Sound")
    SoundSFX.Parent = workspace
    SoundSFX.SoundId = getcustomasset("Azc Hub/" .. soundName)
    SoundSFX:Play()
end

xpcall(function()
    if not(oneclick or kaitan or kaitun or rollback) and not(skip_loading) then
        local LoaderLib = module
        if not(mute_sound) then
            task.spawn(function()
                playSound("Entro Sound", "https://github.com/thecomplexjason/FR3ON/raw/refs/heads/main/Entro%20Sound.mp3")
            end)
        end
        task.wait(0.8)
        LoaderLib:LoadInit({
            Color = Color3.fromRGB(40, 40, 40),
            Duration = 1.5,
            Size = 87,
        })
    end
end, function(err)
    warn(string.format("AZC HUB ERROR: %s", err))
end)
