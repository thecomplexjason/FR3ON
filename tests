autoLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
autoLabel.BackgroundTransparency = 1
autoLabel.TextScaled = true
autoLabel.TextXAlignment = Enum.TextXAlignment.Left

local autoSwitch = Instance.new("TextButton", autoSection)
autoSwitch.Size = UDim2.new(0, 60, 0, 30)
autoSwitch.Position = UDim2.new(1, -70, 0, 10)
autoSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
autoSwitch.Text = ""
autoSwitch.AutoButtonColor = false
autoSwitch.BorderSizePixel = 0

local autoCircle = Instance.new("Frame", autoSwitch)
autoCircle.Size = UDim2.new(0, 26, 0, 26)
autoCircle.Position = UDim2.new(0, 2, 0, 2)
autoCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
