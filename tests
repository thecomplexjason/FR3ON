local infiniteJumpEnabled = false
local jumpLabel = Instance.new("TextLabel", playerSection)
jumpLabel.Size = UDim2.new(0.6, 0, 0, 40)
jumpLabel.Position = UDim2.new(0, 10, 0, 320)
jumpLabel.Text = "نط لانهائي"
jumpLabel.Font = Enum.Font.SourceSansBold
jumpLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
jumpLabel.BackgroundTransparency = 1
jumpLabel.TextScaled = true
jumpLabel.TextXAlignment = Enum.TextXAlignment.Left

local jumpSwitch = Instance.new("TextButton", playerSection)
jumpSwitch.Size = UDim2.new(0, 60, 0, 30)
jumpSwitch.Position = UDim2.new(1, -70, 0, 325)
jumpSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
jumpSwitch.Text = ""
jumpSwitch.AutoButtonColor = false
jumpSwitch.BorderSizePixel = 0

local jumpCircle = Instance.new("Frame", jumpSwitch)
jumpCircle.Size = UDim2.new(0, 26, 0, 26)
jumpCircle.Position = UDim2.new(0, 2, 0, 2)
jumpCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
jumpCircle.BorderSizePixel = 0

Instance.new("UICorner", jumpSwitch).CornerRadius = UDim.new(1, 0)
Instance.new("UICorner", jumpCircle).CornerRadius = UDim.new(1, 0)

local function updateInfiniteJump(state)
	infiniteJumpEnabled = state
	if state then
		jumpSwitch.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
		jumpCircle:TweenPosition(UDim2.new(1, -28, 0, 2), "Out", "Quad", 0.2, true)
	else
		jumpSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
		jumpCircle:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Quad", 0.2, true)
	end
end

jumpSwitch.MouseButton1Click:Connect(function()
	updateInfiniteJump(not infiniteJumpEnabled)
end)

UIS.JumpRequest:Connect(function()
	if infiniteJumpEnabled then
		local char = player.Character
		if char then
			local humanoid = char:FindFirstChildOfClass("Humanoid")
			if humanoid then
				humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
			end
		end
	end
end)
