local noclipEnabled = false
local changedParts = {}

local noclipLabel = Instance.new("TextLabel", playerSection)
noclipLabel.Size = UDim2.new(0.6, 0, 0, 40)
noclipLabel.Position = UDim2.new(0, 10, 0, 500)
noclipLabel.Text = "تعدية الحوائط"
noclipLabel.Font = Enum.Font.SourceSansBold
noclipLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
noclipLabel.BackgroundTransparency = 1
noclipLabel.TextScaled = true
noclipLabel.TextXAlignment = Enum.TextXAlignment.Left

local noclipSwitch = Instance.new("TextButton", playerSection)
noclipSwitch.Size = UDim2.new(0, 60, 0, 30)
noclipSwitch.Position = UDim2.new(1, -70, 0, 505)
noclipSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
noclipSwitch.Text = ""
noclipSwitch.AutoButtonColor = false
noclipSwitch.BorderSizePixel = 0

local noclipCircle = Instance.new("Frame", noclipSwitch)
noclipCircle.Size = UDim2.new(0, 26, 0, 26)
noclipCircle.Position = UDim2.new(0, 2, 0, 2)
noclipCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
noclipCircle.BorderSizePixel = 0

Instance.new("UICorner", noclipSwitch).CornerRadius = UDim.new(1, 0)
Instance.new("UICorner", noclipCircle).CornerRadius = UDim.new(1, 0)

local function updateNoclip(state)
	noclipEnabled = state
	if state then
		noclipSwitch.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
		noclipCircle:TweenPosition(UDim2.new(1, -28, 0, 2), "Out", "Quad", 0.2, true)
	else
		noclipSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
		noclipCircle:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Quad", 0.2, true)

		for part, oldState in pairs(changedParts) do
			if part and part.Parent then
				part.CanCollide = oldState
			end
		end
		changedParts = {}
	end
end

noclipSwitch.MouseButton1Click:Connect(function()
	updateNoclip(not noclipEnabled)
end)

game:GetService("RunService").Stepped:Connect(function()
	if noclipEnabled and player.Character then
		for _, part in pairs(player.Character:GetDescendants()) do
			if part:IsA("BasePart") and part.CanCollide == true then
				changedParts[part] = true
				part.CanCollide = false
			end
		end
	end
end)

