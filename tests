-- ESP للكائنات
local function createCreatureESP(model)
    local adornee = model:FindFirstChildWhichIsA("BasePart")
    if not adornee then return end

    local billboardGui = Instance.new("BillboardGui")
    billboardGui.Size = UDim2.new(0, 150, 0, 30)
    billboardGui.Adornee = adornee
    billboardGui.AlwaysOnTop = true
    billboardGui.ExtentsOffsetWorldSpace = Vector3.new(0, 2, 0)
    billboardGui.Parent = adornee

    local label = Instance.new("TextLabel", billboardGui)
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = model.Name .. "\n AZC Hub - FR3ON"
    label.Font = Enum.Font.SourceSansBold
    label.TextColor3 = Color3.fromRGB(0, 255, 0)
    label.TextScaled = true
    label.TextStrokeTransparency = 0.5
    label.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)

    creatureEspGuis[model] = billboardGui
end

local function startCreatureESP()
    if next(activeCreatures) == nil then
        return
    end
    if creatureESPConnection then
        creatureESPConnection:Disconnect()
        creatureESPConnection = nil
    end
    creatureESPConnection = RunService.RenderStepped:Connect(function()
        local charactersFolder = workspace:FindFirstChild("Characters")
        if not charactersFolder then return end
        for _, creature in pairs(charactersFolder:GetChildren()) do
            if creature:IsA("Model") and activeCreatures[creature.Name] then
                if not creatureEspGuis[creature] then
                    createCreatureESP(creature)
                end
            end
        end
    end)
end

local function stopCreatureESP()
    if creatureESPConnection then
        creatureESPConnection:Disconnect()
        creatureESPConnection = nil
    end
    for model, gui in pairs(creatureEspGuis) do
        if gui then gui:Destroy() end
    end
    creatureEspGuis = {}
end

local function updateCreatureESP(state)
    creatureEspEnabled = state
    if state then
        creatureSwitch.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        creatureCircle:TweenPosition(UDim2.new(1, -28, 0, 2), "Out", "Quad", 0.2, true)
        startCreatureESP()
    else
        creatureSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
        creatureCircle:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Quad", 0.2, true)
        stopCreatureESP()
    end
end

creatureSwitch.MouseButton1Click:Connect(function()
    updateCreatureESP(not creatureEspEnabled)
end)

