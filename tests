local function updateInfo()
    local infoText = ""

    infoText = infoText .. "اللاعبين:\n"
    for _, p in ipairs(Players:GetPlayers()) do
        infoText = infoText .. "- " .. p.Name .. "\n"
    end
    infoText = infoText .. "\n"

    local fireLevelText = "غير معروف"
    local fireLabel = Workspace:FindFirstChild("Map")
        and Workspace.Map:FindFirstChild("Campground")
        and Workspace.Map.Campground:FindFirstChild("MainFire")
        and Workspace.Map.Campground.MainFire:FindFirstChild("Center")
        and Workspace.Map.Campground.MainFire.Center:FindFirstChild("BillboardGui")
        and Workspace.Map.Campground.MainFire.Center.BillboardGui:FindFirstChild("Frame")
        and Workspace.Map.Campground.MainFire.Center.BillboardGui.Frame:FindFirstChild("TextLabel")

    if fireLabel then
        if fireLabel.Text == " " or not fireLabel.Visible then
            fireLevelText = "6"
        else
            local fullText = fireLabel.Text
            if #fullText >= 7 then
                fireLevelText = string.sub(fullText, 7, 7)
                if tonumber(fireLevelText) == nil then
                    fireLevelText = "يتم التحديد الان..."
                end
            else
                fireLevelText = "غير مشتعلة"
            end
        end
    end
    infoText = infoText .. "لفل النار: " .. fireLevelText .. "\n\n"

    local scrapCount = "فشل التحديد"
    local scrapLabel = Workspace:FindFirstChild("Map")
        and Workspace.Map:FindFirstChild("Campground")
        and Workspace.Map.Campground:FindFirstChild("CraftingBench")
        and Workspace.Map.Campground.CraftingBench:FindFirstChild("ScrapSign")
        and Workspace.Map.Campground.CraftingBench.ScrapSign:FindFirstChild("SurfaceGui")
        and Workspace.Map.Campground.CraftingBench.ScrapSign.SurfaceGui:FindFirstChild("TextLabel")

    if scrapLabel and tonumber(scrapLabel.Text) ~= nil then
        scrapCount = scrapLabel.Text
    end
    infoText = infoText .. "عدد الحديد: " .. scrapCount .. "\n\n"

    local woodCount = "فشل التحديد"
    local woodLabel = Workspace:FindFirstChild("Map")
        and Workspace.Map:FindFirstChild("Campground")
        and Workspace.Map.Campground:FindFirstChild("CraftingBench")
        and Workspace.Map.Campground.CraftingBench:FindFirstChild("WoodSign")
        and Workspace.Map.Campground.CraftingBench.WoodSign:FindFirstChild("SurfaceGui")
        and Workspace.Map.Campground.CraftingBench.WoodSign.SurfaceGui:FindFirstChild("TextLabel")

    if woodLabel then
        local cleanedText = woodLabel.Text:gsub("%D", "")
        if cleanedText ~= "" then
            woodCount = cleanedText
        end
    end
    infoText = infoText .. "عدد الخشب: " .. woodCount .. "\n\n"

    local currentAxe = "لا يوجد"
    local axes = {"Old Axe", "Good Axe", "Strong Axe", "Chainsaw"}
    local inventory = localPlayer:FindFirstChild("Inventory")
    if inventory then
        for _, item in ipairs(inventory:GetChildren()) do
            if table.find(axes, item.Name) then
                currentAxe = item.Name
                break
            end
        end
    end
    infoText = infoText .. "الفأس الحالي: " .. currentAxe .. "\n\n"

    local currentBag = "لا يوجد"
    local bags = {"Giant Sack", "Good Sack", "Old Sack"}
    if inventory then
        for _, item in ipairs(inventory:GetChildren()) do
            if table.find(bags, item.Name) then
                currentBag = item.Name
                break
            end
        end
    end
    infoText = infoText .. "الكيس الحالي: " .. currentBag .. "\n"

    infoLabel.Text = infoText
end

task.spawn(function()
    while true do
        updateInfo()
        task.wait(1)
    end
end)


