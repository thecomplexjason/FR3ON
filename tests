local MIN_FORCE   = 50
local MAX_FORCE   = 1000
local START_FORCE = 300
local TWEEN_TIME  = 0.18

local tpJumpEnabled = false
local slowJumpForce = START_FORCE
local originalGravity = Workspace.Gravity

local tpJumpLabel = Instance.new("TextLabel", playerSection)
tpJumpLabel.Size = UDim2.new(0.6, 0, 0, 40)
tpJumpLabel.Position = UDim2.new(0, 10, 0, 260)
tpJumpLabel.Text = "نط عالي"
tpJumpLabel.Font = Enum.Font.SourceSansBold
tpJumpLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
tpJumpLabel.BackgroundTransparency = 1
tpJumpLabel.TextScaled = true
tpJumpLabel.TextXAlignment = Enum.TextXAlignment.Left

local tpJumpSwitch = Instance.new("TextButton", playerSection)
tpJumpSwitch.Size = UDim2.new(0, 60, 0, 30)
tpJumpSwitch.Position = UDim2.new(1, -70, 0, 265)
tpJumpSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
tpJumpSwitch.Text = ""
tpJumpSwitch.AutoButtonColor = false
tpJumpSwitch.BorderSizePixel = 0

local tpJumpCircle = Instance.new("Frame", tpJumpSwitch)
tpJumpCircle.Size = UDim2.new(0, 26, 0, 26)
tpJumpCircle.Position = UDim2.new(0, 2, 0, 2)
tpJumpCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
tpJumpCircle.BorderSizePixel = 0
Instance.new("UICorner", tpJumpSwitch).CornerRadius = UDim.new(1, 0)
Instance.new("UICorner", tpJumpCircle).CornerRadius = UDim.new(1, 0)

local function updateTpJump(state)
	tpJumpEnabled = state
	if state then
		tpJumpSwitch.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
		tpJumpCircle:TweenPosition(UDim2.new(1, -28, 0, 2), "Out", "Quad", 0.2, true)
	else
		tpJumpSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
		tpJumpCircle:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Quad", 0.2, true)
	end
end

tpJumpSwitch.MouseButton1Click:Connect(function()
	updateTpJump(not tpJumpEnabled)
end)

UIS.JumpRequest:Connect(function()
	if tpJumpEnabled then
		local char = player.Character
		local humanoid = char and char:FindFirstChildWhichIsA("Humanoid")
		if char and humanoid and humanoid:GetState() ~= Enum.HumanoidStateType.Freefall then
			local hrp = char:FindFirstChild("HumanoidRootPart")
			if hrp then
				hrp.AssemblyLinearVelocity = Vector3.new(
					hrp.AssemblyLinearVelocity.X,
					slowJumpForce / hrp.Mass,
					hrp.AssemblyLinearVelocity.Z
				)
			end
		end
	end
end)

local sliderCard = Instance.new("Frame", playerSection)
sliderCard.Size = UDim2.new(0.9, 0, 0, 80)
sliderCard.Position = UDim2.new(0.05, 0, 0, 10)
sliderCard.BackgroundColor3 = Color3.fromRGB(18,18,18)
sliderCard.BackgroundTransparency = 1
Instance.new("UICorner", sliderCard).CornerRadius = UDim.new(0, 12)

local sliderTitle = Instance.new("TextLabel", sliderCard)
sliderTitle.Size = UDim2.new(1, -20, 0, 28)
sliderTitle.Position = UDim2.new(0, 10, 0, 5)
sliderTitle.BackgroundTransparency = 1
sliderTitle.Font = Enum.Font.GothamMedium
sliderTitle.Text = "= قوة النط العالي الحالية"
sliderTitle.TextSize = 18
sliderTitle.TextColor3 = Color3.fromRGB(235,235,235)
sliderTitle.TextXAlignment = Enum.TextXAlignment.Left

local valueLabel = Instance.new("TextLabel", sliderCard)
valueLabel.Size = UDim2.new(0, 60, 0, 28)
valueLabel.Position = UDim2.new(1, -65, 0, 5)
valueLabel.BackgroundTransparency = 1
valueLabel.Font = Enum.Font.Gotham
valueLabel.Text = tostring(slowJumpForce)
valueLabel.TextSize = 16
valueLabel.TextColor3 = Color3.fromRGB(235,235,235)
valueLabel.TextXAlignment = Enum.TextXAlignment.Right

local track = Instance.new("Frame", sliderCard)
track.Size = UDim2.new(1, -40, 0, 12)
track.Position = UDim2.new(0, 20, 0, 45)
track.BackgroundColor3 = Color3.fromRGB(34,34,34)
Instance.new("UICorner", track).CornerRadius = UDim.new(0, 999)

local trackGrad = Instance.new("UIGradient", track)
trackGrad.Color = ColorSequence.new({
	ColorSequenceKeypoint.new(0, Color3.fromRGB(34,34,34)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(64,64,64))
})

local fill = Instance.new("Frame", track)
fill.Size = UDim2.new((slowJumpForce - MIN_FORCE)/(MAX_FORCE-MIN_FORCE), 0, 1, 0)
fill.BackgroundColor3 = Color3.fromRGB(240,240,240)
Instance.new("UICorner", fill).CornerRadius = UDim.new(0, 999)

local fillGrad = Instance.new("UIGradient", fill)
fillGrad.Color = ColorSequence.new({
	ColorSequenceKeypoint.new(0, Color3.fromRGB(240,240,240)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(160,160,160))
})

local knob = Instance.new("Frame", track)
knob.Size = UDim2.new(0, 22, 0, 22)
knob.AnchorPoint = Vector2.new(0.5, 0.5)
knob.Position = UDim2.new((slowJumpForce - MIN_FORCE)/(MAX_FORCE-MIN_FORCE), 0, 0.5, 0)
knob.BackgroundColor3 = Color3.fromRGB(255,255,255)
Instance.new("UICorner", knob).CornerRadius = UDim.new(0, 999)

local knobGrad = Instance.new("UIGradient", knob)
knobGrad.Rotation = 90
knobGrad.Color = ColorSequence.new({
	ColorSequenceKeypoint.new(0, Color3.fromRGB(255,255,255)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(190,190,190))
})

local dragging = false

local function clamp(n, lo, hi)
	if n < lo then return lo end
	if n > hi then return hi end
	return n
end

local function forceFromPercent(p)
	return math.floor(MIN_FORCE + p * (MAX_FORCE - MIN_FORCE) + 0.5)
end

local function tweenTo(guiObject, props)
	TweenService:Create(guiObject, TweenInfo.new(TWEEN_TIME, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), props):Play()
end

local function applyVisuals(p)
	local px = p * track.AbsoluteSize.X
	tweenTo(fill, { Size = UDim2.new(0, px, 1, 0) })
	tweenTo(knob, { Position = UDim2.new(0, px, 0.5, 0) })
	valueLabel.Text = tostring(forceFromPercent(p))
end

local function setPercentFromInput(x)
	local trackAbsX = track.AbsolutePosition.X
	local trackAbsW = track.AbsoluteSize.X
	local p = (x - trackAbsX) / math.max(trackAbsW, 1)
	p = clamp(p, 0, 1)

	slowJumpForce = forceFromPercent(p)
	applyVisuals(p)
end

track.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		setPercentFromInput(UserInputService:GetMouseLocation().X)
	end
end)

UserInputService.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = false
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
		setPercentFromInput(UserInputService:GetMouseLocation().X)
	end
end)

