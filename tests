local interface = playerGui:FindFirstChild("Interface")
local fishingCatchFrame = interface and interface:FindFirstChild("FishingCatchFrame")
local timingBar = fishingCatchFrame and fishingCatchFrame:FindFirstChild("TimingBar")
local bar = timingBar and timingBar:FindFirstChild("Bar")
local successArea = timingBar and timingBar:FindFirstChild("SuccessArea")

local autoConnection

local function startAuto()
	if autoConnection then autoConnection:Disconnect() end
	if not timingBar or not successArea then return end
	autoConnection = RunService.RenderStepped:Connect(function()
		if timingBar and successArea then
			successArea.Size = UDim2.new(1, 0, 1, 0)
			successArea.Position = UDim2.new(0, 0, 0, 0)
			successArea.AnchorPoint = Vector2.new(0,0)
		end
	end)
end

local function stopAuto()
	if autoConnection then
		autoConnection:Disconnect()
		autoConnection = nil
	end
end

local function updateAuto(state)
	autoEnabled = state
	if state then
		autoSwitch.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
		autoCircle:TweenPosition(UDim2.new(1, -28, 0, 2), "Out", "Quad", 0.2, true)
		startAuto()
	else
		autoSwitch.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
		autoCircle:TweenPosition(UDim2.new(0, 2, 0, 2), "Out", "Quad", 0.2, true)
		stopAuto()
	end
end

autoSwitch.MouseButton1Click:Connect(function()
	updateAuto(not autoEnabled)
end)

